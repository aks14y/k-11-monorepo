# Environment Variables Example
# Copy this file to .env and adjust values as needed
# Use dist/ files instead of src/ (true/false)
# Development: false (uses src/ for faster HMR)
# Production: true (uses dist/ for optimized builds)
USE_DIST=false
# Port for development server
PORT=3000
# Feature Flags - Enable/Disable MFEs (true/false)
ENABLE_K11_INBOX=true
ENABLE_K11_MONITORING=true
