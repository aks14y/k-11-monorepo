{
  "name": "shell",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack serve --config webpack.config.js --env development",
    "dev:dist": "USE_DIST=true webpack serve --config webpack.config.js --env development",
    "build": "webpack --config webpack.config.js --env production",
    "build:analyze": "webpack --config webpack.config.js --env production --env analyze",
    "build:stats": "webpack --config webpack.config.js --env production --json > dist/bundle-stats.json",
    "analyze": "webpack-bundle-analyzer dist/bundle-stats.json || echo 'Run pnpm build:stats first to generate bundle-stats.json'",
    "serve": "serve dist -l 3000",
    "serve:prod": "serve dist -l 3000 --single",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@design-system": "workspace:*",
    "@emotion/is-prop-valid": "^1.3.1",
    "@emotion/memoize": "^0.8.1",
    "@emotion/unitless": "^0.8.1",
    "k11-inbox": "workspace:*",
    "k11-monitoring": "workspace:*",
    "@remix-run/router": "^1.19.1",
    "@types": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.21.3",
    "scheduler": "^0.23.2",
    "shallowequal": "^1.1.0",
    "styled-components": "^6.1.0",
    "stylis": "^4.3.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.1.1",
    "events": "^3.3.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "serve": "^14.2.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.90.0",
    "webpack-bundle-analyzer": "^5.0.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
